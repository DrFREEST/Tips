      "animation-delay",
      "animation-iteration-count",
      "animation-iteration-count",
      "animation-direction",
      "text-align",
      "text-align-last",
      "vertical-align",
      "white-space",
      "text-decoration",
      "text-emphasis",
      "text-emphasis-color",
